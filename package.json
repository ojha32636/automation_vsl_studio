{
  "name": "vsl",
  "version": "1.0.0",
  "description": "Vsl test",
  "main": "index.js",
  "git": "https://github.com/ojha32636/vslstudio",
  "scripts": {
    "pretest": "rimraf -r ./cypress/reports/*.json",
    "test": "npm run cypress-test || npm run posttest",
    "cypress-test": "cypress run",
    "posttest": "npm run merge-reports && npm run generate-htmlreport",
    "merge-reports": "mochawesome-merge ./cypress/reports/*.json > ./report1.json",
    "generate-htmlreport": "marge report1.json --reportDir ./cypress/reports"
  },
  "author": "Omprakash jha",
  "license": "ISC",
  "dependencies": {
    "cypress": "^9.6.0",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.11.1",
    "cypress-xpath": "^1.6.2",
    "mochawesome-report-generator": "^6.2.0"
  }
}
